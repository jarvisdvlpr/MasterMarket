@{
    ViewData["Title"] = "Home Page";
}
@model HomeViewModel
 <!-- Product Section Start -->
    <section class="product-section pt-0">
        <div class="container-fluid p-0">
            <div class="custom-row">
                <div class="sidebar-col">
                    <div class="category-menu">
                        <a asp-controller="Home" asp-action="index" class="nav-logo">
                            <img src="~/assets/images/logo/1.png" class="img-fluid blur-up lazyload" alt="">
                        </a>
                        
                        <ul>
                            @foreach (var item in Model.Categories)
                        {
                            <li>
                                <div class="category-list">
                                    <img src="~/uploads/category/@item.IconSrc" class="blur-up lazyload" alt="">
                                    <h5>
                                        <a asp-action="index" asp-controller="category" asp-route-id="@item.Id">@item.Name</a>
                                    </h5>
                                </div>
                            </li>
                        }
                        </ul>
                    </div>
                </div>

                <div class="content-col">
                    <div class="section-b-space">
                        <div class="col-12">
                            
                                <div class="banner-contain hover-effect">
                                    <img src="~/assets/images/firstbanner.jpg" class="bg-img blur-up lazyload"
                                        alt="">
